{% embed "@templates/_default.twig" with {
  page_header: TRUE,
  full_width: true
}%}

  {% block page_header %}
    {% include "@organisms/basic-header/basic-header.twig" with {
      breadcrumb: header.breadcrumb
    } %}
  {% endblock %}

  {% block page_content %}
    {% include "@organisms/box-links/box-links.twig" with {
      boxes: boxes
    } %}
    
    {% include "@organisms/service-tile-listing/service-tile-listing.twig" with {
      header: services_header,
      lede: services_lede
    } %}
    
    {% include "@organisms/anchored-cta-block/anchored-cta-block.twig" with {
      "title": cta.title,
      "text": cta.text,
      "link": cta.link
    } %}
  {% endblock %}
{% endembed %}
