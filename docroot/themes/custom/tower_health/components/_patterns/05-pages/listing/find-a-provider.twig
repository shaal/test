{% embed "@templates/_default.twig" with {
  page_header: TRUE,
  full_width: true
}%}

  {% block page_header %}
    {% include "@organisms/basic-header/basic-header.twig" with {
      breadcrumb: header.breadcrumb,
      class: header.class,
    } %}
  {% endblock %}

  {% block page_content %}
    {% include "@organisms/provider-search/provider-search.twig" %}
    {% include "@organisms/provider-filter/provider-filter.twig" %}
    {% include "@organisms/provider-list/provider-list.twig" with {
        "providers": providers,
      } %}
    {% include "@molecules/pagination/pagination.twig" %}
    {% include "@organisms/listing-cta/listing-cta.twig" with {
        "title": cta.title,
        "text": cta.text,
        "link": cta.link
      } %}
  {% endblock %}

{% endembed %}
